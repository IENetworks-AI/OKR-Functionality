# Environment Variables Configuration
# Copy this file to .env.local and fill in your actual values

# Firebase Authentication
VITE_EMAIL=your-email@example.com
VITE_PASSWORD=your-password
VITE_FIREBASE_API_KEY=your-firebase-api-key

# API Configuration
VITE_API_BASE_URL=https://your-api-base-url.com
VITE_TENANT_ID=your-tenant-id

# Plan IDs
VITE_DAILY_PLAN_ID=your-daily-plan-id
VITE_WEEKLY_PLAN_ID=your-weekly-plan-id

# AI Model Configuration (for Netlify functions)
MODEL_API_URL=https://generativelanguage.googleapis.com/v1beta/models/gemini-1.5-flash-latest:generateContent
MODEL_API_KEY=your-google-ai-api-key